{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    // HERE
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "prettier"],
  
    "rules": {
      "no-unused-vars": "error",
      "no-console" : "warn",
      "no-undef" : "error",
      "no-unused-expressions" : "error",
      "no-redeclare" : "error",
      // "consistent-type-definitions": ["error", "type","import"]
    },
  
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "globals": {
      "process" : "readonly"
    }
  }